<!DOCTYPE web-app PUBLIC
        "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
        "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>

  <servlet>
    <servlet-name>hello</servlet-name>
    <servlet-class>servlet.HelloServlet</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>date</servlet-name>
    <servlet-class>servlet.DateServlet</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>topic</servlet-name>
    <servlet-class>servlet.TopicServlet</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>post</servlet-name>
    <servlet-class>servlet.PostServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>hello</servlet-name>
    <url-pattern>/hello</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>date</servlet-name>
    <url-pattern>/date</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>topic</servlet-name>
    <url-pattern>/topic</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>post</servlet-name>
    <url-pattern>/post</url-pattern>
  </servlet-mapping>

</web-app>




